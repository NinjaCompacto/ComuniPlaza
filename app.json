{
  "expo": {
    "name": "ComuniPlaza",
    "slug": "ComuniPlaza",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/logo_azul.png",
    "userInterfaceStyle": "light",
    "scheme": "ComuniPlaza",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_a.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.jvnsouza2003.ComuniPlaza"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa de permissão para acessar suas fotos.",
          "cameraPermission": "O aplicativo precisa de permissão para acessar sua câmera."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "797464e3-de5f-4677-b1f8-e33b52f4a028"
      }
    }
  }
}
